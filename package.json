    "test": "karma start build/karma.conf.js",
    "update-webdriver": "webdriver-manager update",
    "watch": "gulp watch",
    "watch:test": "karma start tools/karma.conf.js",
    "list-e2e": "node ./test/e2e/list-tests.js ./build/protractor.conf.js"
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.js --ssl 1 --ssl-key dev-ssl/server.key --ssl-cert dev-ssl/server.crt",
    "build": "ng build",
    "test-debug": "ng test --sourcemaps=false",
    "test": "ng test --code-coverage --sourcemaps=false -sr",
    "posttest": "codecov -f coverage/*.json",
    "lint": "ng lint --type-check --format stylish",
    "sass-lint": "sass-lint -v",
    "e2e": "ng e2e --proxy-config proxy.conf.js --ssl 1 --ssl-key dev-ssl/server.key --ssl-cert dev-ssl/server.crt",
    "gate-check": "npm run lint && npm run test-debug -- --browsers ChromeHeadless --single-run",
    "climate": "codeclimate analyze $(git diff --name-only master)"
