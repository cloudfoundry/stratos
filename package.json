    "@ngrx/store-devtools": "^14.3.3",
    "@swimlane/ngx-charts": "^20.4.1",
    "@swimlane/ngx-graph": "^8.2.2",
    "@types/moment-timezone": "^0.5.13",
    "angular2-virtual-scroll": "^0.4.16",
    "core-js": "^3.6.5",
    "immer": "^10.0.3",
    "intersect": "^1.0.1",
    "lodash": "^4.17.20",
    "lodash-es": "^4.17.14",
    "mappy-breakpoints": "^0.2.3",
    "marked": "^4.3.0",
    "moment": "^2.24.0",
