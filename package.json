    "@angular/cli": "^14.2.12",
    "@angular/language-service": "^14.2.12",
    "@cypress/request": "^3.0.0",
    "@schematics/angular": "^9.1.5",
    "@types/hammerjs": "^2.0.46",
    "@types/jasmine": "~3.6.0",
