    "karma-spec-reporter": "0.0.32",
    "kind-of": "^6.0.3",
    "mem": "6.1.0",
    "mktemp": "^1.0.0",
    "ng-packagr": "^14.2.2",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "protractor": "^7.0.0",
    "ps-node": "^0.1.6",
